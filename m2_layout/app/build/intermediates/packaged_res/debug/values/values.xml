<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">HomeWork</string>
    <style name="CustomViewText">
        <item name="android:padding">16dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Theme.HomeWork" parent="android:Theme.Material.Light.NoActionBar"/>
    <declare-styleable name="CustomView">
        <attr format="reference" name="id"/>
        <attr format="string" name="first_row_text"/>
        <attr format="string" name="second_row_text"/>
        <attr format="color" name="frame_background_color"/>
    </declare-styleable>
</resources>