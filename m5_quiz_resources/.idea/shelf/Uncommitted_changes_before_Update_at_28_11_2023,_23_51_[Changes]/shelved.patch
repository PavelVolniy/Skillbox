+package com.example.quizapp
+
+object QuizStorage {
+    fun getQuiz(locale: Locale): Quiz = when (locale) {
+        Locale.Ru -> quizRu
+        Locale.En -> quizEn
+    }
+
+    fun answer(quiz: Quiz, answers: List<Int>): String = quiz
+        .questions
+        .zip(answers) { question, index -> question.feedback[index] }
+        .joinToString(separator = " ")
